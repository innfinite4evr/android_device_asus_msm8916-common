# healthd seclabel is specified in init.rc 
# since it lives in the rootfs and has no 
# unique file type.
allow healthd device:dir r_dir_perms;
allow healthd self:capability { mknod };
allow healthd device:dir { add_name write };
allow healthd device:dir create_dir_perms;
allow healthd device:file create_file_perms;
